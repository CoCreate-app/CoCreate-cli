    "mini-css-extract-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.1.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0",
    "webpack-log": "^3.0.1"
  },
  "dependencies": {
    "@cocreate/docs": "^1.5.1",
    "@cocreate/hosting": "^1.7.1",
    "colors": "latest",
    "glob": "^7.1.7",
